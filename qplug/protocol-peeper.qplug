-- The runtime code
if Controls then
  -- Runtime Lua code goes here
  rapidjson = require("rapidjson")
  EzSVG = require "EzSVG"

  -- Configuration
  local dim = 1000
  local lineHeight = 43
  local fontSize = 32
  local maxLines = math.floor(dim / lineHeight) -- Number of visible lines
  local textBuffer = {}                         -- Store incoming lines of text
  local maxBufferSize = 500                     -- Increased to store 500 lines
  local timestampWidth = 150                    -- Increased width for timestamps (HH:MM:SS)
  local updateInterval = 0.001                  -- Time between updates in seconds
  local currentScrollPos = 0                    -- Track scroll position (0 means viewing latest)
  local currentFilter = ""                      -- Store current filter text
  local pendingBuffer = {}                      -- Buffer for data received before client connection

  local charsPerLine = 35

  -- Add at the top with other configuration
  local pendingBufferTimer = Timer.New()
  local pendingBufferTimeout = 10  -- seconds to wait before clearing buffer
  local processTimer = Timer.New() -- Add this back

  -- Create SVG document
  local svg = EzSVG.Document(dim, dim, "white")

  -- Message queue for processing
  local messageQueue = {}

  -- Message formats with darker colors
  local MessageFormat = {
    OUTGOING = {
      text_anchor = "start", -- Left justified
      text_color = "black",
      background = "#C6E0B4" -- Darker green
    },
    INCOMING = {
      text_anchor = "end",   -- Right justified
      text_color = "black",
      background = "#B4C7E7" -- Darker blue
    },
    STATUS = {
      text_anchor = "middle", -- Centered
      text_color = "black",
      background = "#FFE699"  -- Darker yellow
    },
    ERROR = {
      text_anchor = "middle", -- Centered
      text_color = "#CC0000", -- Red text
      background = "#FFB6B6"  -- Darker red
    }
  }

  local FormatLabel = {
    [MessageFormat.OUTGOING] = ">>",
    [MessageFormat.INCOMING] = "<<",
    [MessageFormat.STATUS] = "**",
    [MessageFormat.ERROR] = "!!"
  }

  -- Function to format timestamp (HH:MM:SS)
  local function formatTimestamp()
    local time = os.date("*t")
    return string.format("%02d:%02d:%02d", time.hour, time.min, time.sec)
  end

  -- Function to convert non-printable chars to hex and split into tokens
  local function tokenizeWithHex(str)
    local tokens = {}
    local pos = 1

    while pos <= #str do
      local c = str:sub(pos, pos)
      local byte = string.byte(c)
      if byte < 32 or byte > 126 then
        -- Non-printable character - convert to hex token
        table.insert(tokens, string.format("[%02X]", byte))
        pos = pos + 1
      else
        -- Printable character - add as single char token
        table.insert(tokens, c)
        pos = pos + 1
      end
    end

    return tokens
  end

  -- Function to wrap text into lines that fit within the width
  local function wrapText(text, format)
    local lines = {}
    local tokens = tokenizeWithHex(text)
    local currentLine = ""
    local pos = 1
    if format == MessageFormat.INCOMING or format == MessageFormat.OUTGOING then
      while pos <= #tokens do
        local token = tokens[pos]
        -- Check if adding this token would exceed line length
        if #currentLine + #token <= charsPerLine then
          currentLine = currentLine .. token
          pos = pos + 1
        else
          -- Line would be too long - start new line
          table.insert(lines, currentLine)
          currentLine = ""
        end
      end

      -- Add final line if not empty
      if currentLine ~= "" then
        table.insert(lines, currentLine)
      end
    else
      table.insert(lines, text)
    end
    return lines
  end


  -- Function to escape XML special characters
  local function escapeXML(str)
    local replacements = {
      ["<"] = "&lt;",
      [">"] = "&gt;",
      ["&"] = "&amp;",
      ['"'] = "&quot;",
      ["'"] = "&apos;"
    }

    -- Process & first to avoid double-escaping
    str = str:gsub("&", replacements["&"])
    -- Then handle other special characters
    str = str:gsub("[<>\"']", replacements)
    return str
  end

  -- Function to process a single message from the queue
  local function processMessage()
    if #messageQueue > 0 then
      local message = table.remove(messageQueue, 1)

      local wrappedLines = wrapText(message.text, message.format)
      -- Add first line with timestamp, escaping XML after wrapping
      table.insert(textBuffer, {
        timestamp = message.timestamp,
        text = escapeXML(wrappedLines[1] or ""),
        format = message.format
      })

      -- Add subsequent lines with empty timestamp area
      for i = 2, #wrappedLines do
        table.insert(textBuffer, {
          timestamp = "",
          text = escapeXML(wrappedLines[i]),
          format = message.format
        })
      end

      -- Remove old lines if buffer is too large
      while #textBuffer > maxBufferSize do
        table.remove(textBuffer, 1)
      end

      -- Update display
      updateDisplay()
    else
      -- Stop timer when message queue is empty
      processTimer:Stop()
    end
  end

  -- Modify addText function
  local function addText(message, format)
    -- Special case for status messages about capture state

    if Controls.capture.Boolean or (message == "Capture Enabled" or message == "Capture Disabled") then
      currentScrollPos = 0 -- Reset scroll position when capturing
      -- Get current timestamp
      local timestamp = formatTimestamp()

      -- Print formatted message
      local label = FormatLabel[format] or "??"
      print(string.format("%s %s", label, message))
      Controls.output.String = string.format("%s %s %s", timestamp, label, message)

      -- Add message to queue
      table.insert(messageQueue, {
        timestamp = timestamp,
        text = message,
        format = format or MessageFormat.INCOMING -- Default to incoming if no format specified
      })

      -- Start processing if timer isn't running
      if not processTimer:IsRunning() then
        processTimer.EventHandler = processMessage
        processTimer:Start(updateInterval)
      end
    end
  end

  -- Function to update the SVG display
  function updateDisplay()
    -- Create new SVG document each time
    svg = EzSVG.Document(dim, dim, "white")

    -- Build filtered buffer
    local filteredBuffer = {}
    local i = 1 --message index, but indicies i and j could be reworked to make more sense
    while i <= #textBuffer do
      local entry = textBuffer[i]
      -- Check if this is a first line (has timestamp)
      if entry.timestamp ~= "" then
        -- Start of a new message - collect all its lines
        local messageLines = { entry }
        local messageLinesString = entry.text
        local j = i + 1
        while j <= #textBuffer and textBuffer[j].timestamp == "" do
          table.insert(messageLines, textBuffer[j])
          messageLinesString = messageLinesString .. textBuffer[j].text --concat one big string for the message
          j = j + 1
        end

        -- Check if any line in the message matches the filter
        local shouldInclude = false
        if entry.format == MessageFormat.STATUS or entry.format == MessageFormat.ERROR then
          shouldInclude = true
        elseif currentFilter == "" then
          shouldInclude = true
        else
          -- Try as regex first
          local success, result = pcall(function()
            return string.match(messageLinesString, currentFilter) ~= nil
          end)

          -- If regex fails, fall back to substring search
          if not success then
            result = string.find(messageLinesString, currentFilter, 1, true) ~= nil
          end
          if result then
            shouldInclude = true
          end
        end

        -- If any part matched, include all lines
        if shouldInclude then
          for _, line in ipairs(messageLines) do
            table.insert(filteredBuffer, line)
          end
        end

        i = j     -- Move to next message
      else
        i = i + 1 -- Skip orphaned continuation lines
      end
    end

    -- Calculate which lines to show based on scroll position
    local endLine = #filteredBuffer
    if not Controls.capture.Boolean and currentScrollPos > 0 then
      endLine = math.max(1, #filteredBuffer - currentScrollPos)
    end
    local startLine = math.max(1, endLine - maxLines + 1)

    -- Add timestamp background rectangle
    local timestampBg = EzSVG.Rect(0, 0, timestampWidth, dim)
    timestampBg:setStyle({
      fill = "#2D2D2D",
      stroke = "none"
    })
    svg:add(timestampBg)

    -- First pass: Add all background rectangles
    yPos = 0
    for i = startLine, endLine do
      local format = filteredBuffer[i].format or MessageFormat.INCOMING
      if yPos ~= 0 then
        yPos = yPos + 5
      end
      -- Add background for this line (full width)
      local bgRect = EzSVG.Rect(
        timestampWidth,
        yPos,
        dim - timestampWidth,
        lineHeight + 10
      )
      bgRect:setStyle({
        fill = format.background,
        stroke = "none"
      })
      svg:add(bgRect)

      if yPos ~= 0 then
        yPos = yPos + lineHeight - 5
      else
        yPos = yPos + lineHeight
      end
    end

    -- Second pass: Add all text
    yPos = lineHeight -- Reset yPos for text pass
    for i = startLine, endLine do
      local format = filteredBuffer[i].format or MessageFormat.INCOMING

      -- Add timestamp at left edge
      if filteredBuffer[i].timestamp ~= "" then
        local timestamp = EzSVG.Text(filteredBuffer[i].timestamp, 5, yPos)
        timestamp:setStyle({
          font_family = "Courier",
          font_size = fontSize,
          fill = "#E0E0E0",
          text_anchor = "start"
        })
        svg:add(timestamp)
      end

      -- Set text position based on alignment
      local textX
      if format.text_anchor == "end" then
        textX = dim - 5                                     -- Right edge with small offset
      elseif format.text_anchor == "middle" then
        textX = timestampWidth + (dim - timestampWidth) / 2 -- Center of text area
      else
        textX = timestampWidth + 5                          -- Just after timestamp area
      end

      -- Add message text
      local text = EzSVG.Text(filteredBuffer[i].text, textX, yPos)
      text:setStyle({
        font_family = "Consolas",
        font_size = fontSize,
        fill = format.text_color or "black",
        text_anchor = format.text_anchor
      })
      svg:add(text)

      yPos = yPos + lineHeight
    end

    -- Convert to string and update display
    local image = svg:toString("Design\\doc-example.svg")
    Controls.SVG.Legend = rapidjson.encode({
      DrawChrome = false,
      IconData = Crypto.Base64Encode(image),
    })
  end

  -- Add scroll timer and state variables
  local scrollTimer = Timer.New()
  local scrollInterval = 0.1 -- seconds between scroll updates when holding

  -- Modify scroll control functions
  function scrollUp()
    if not Controls.capture.Boolean then
      local maxScroll = math.max(0, #textBuffer - maxLines)
      currentScrollPos = math.min(maxScroll, currentScrollPos + 1)
      updateDisplay()
    end
  end

  function scrollDown()
    if not Controls.capture.Boolean then
      currentScrollPos = math.max(0, currentScrollPos - 1)
      updateDisplay()
    end
  end

  -- Add continuous scroll handlers
  Controls.ScrollUp.EventHandler = function(ctrl)
    if ctrl.Boolean then
      -- Initial scroll
      scrollUp()
      -- Start continuous scrolling
      scrollTimer.EventHandler = scrollUp
      scrollTimer:Start(scrollInterval)
    else
      -- Stop scrolling when button released
      scrollTimer:Stop()
    end
  end

  Controls.ScrollDown.EventHandler = function(ctrl)
    if ctrl.Boolean then
      -- Initial scroll
      scrollDown()
      -- Start continuous scrolling
      scrollTimer.EventHandler = scrollDown
      scrollTimer:Start(scrollInterval)
    else
      -- Stop scrolling when button released
      scrollTimer:Stop()
    end
  end


  -- Add handler for scroll to bottom
  Controls.ScrollToBottom.EventHandler = function(ctrl)
    if ctrl.Boolean then
      currentScrollPos = 0
      updateDisplay()
    end
  end

  -- Reset scroll position when capture is enabled
  function resetScroll()
    currentScrollPos = 0
    updateDisplay()
  end

  function RemoveSocketsFromTable()
    if sockets then
      for k, v in pairs(sockets) do
        v:Disconnect()
        --    if v == sock then
        table.remove(sockets, k)
      end
    end
  end

  function init()
    -- Clear the display
    textBuffer = {}           -- Clear text buffer
    messageQueue = {}         -- Clear message queue
    pendingBuffer = {}        -- Clear pending buffer
    pendingBufferTimer:Stop() -- Stop any existing timer
    currentScrollPos = 0      -- Reset scroll position
    updateDisplay()           -- Update the empty display

    Controls.capture.Boolean = true
    Controls.ScrollUp.IsInvisible = true
    Controls.ScrollDown.IsInvisible = true
    Controls.ScrollToBottom.IsInvisible = true

    ---====================SERVER
    server = TcpSocketServer.New()

    -- table to store connected client sockets
    -- this is required so the sockets don't
    -- get garbage collected since there aren't
    -- any other references to them in the script
    sockets = {}



    function SocketHandler(sock, event) -- the arguments for this EventHandler are documented in the EventHandler definition of TcpSocket Properties
      if event == TcpSocket.Events.Data then
        local data = sock:Read(sock.BufferLength)
        if clientSock.IsConnected then
          clientSock:Write(data)
        else
          -- Store data in pending buffer
          table.insert(pendingBuffer, data)
          addText("Buffering data until client connects...", MessageFormat.STATUS)

          -- Start timeout timer if not already running
          if not pendingBufferTimer:IsRunning() then
            pendingBufferTimer.EventHandler = function()
              if #pendingBuffer > 0 then
                addText(string.format("Buffer timeout - discarding %d messages", #pendingBuffer), MessageFormat.ERROR)
                pendingBuffer = {}
              end
              pendingBufferTimer:Stop()
            end
            pendingBufferTimer:Start(pendingBufferTimeout)
          end
        end
        addText(data, MessageFormat.OUTGOING)
      elseif event == TcpSocket.Events.Closed or
          event == TcpSocket.Events.Error or
          event == TcpSocket.Events.Timeout then
        -- remove reference of socket from table so
        -- it's available for garbage collection
        RemoveSocketsFromTable()
        if clientsock then clientSock:Disconnect() end
      end
    end

    server.EventHandler = function(SocketInstance) -- the properties of this socket instance are those of the TcpSocket library
      SocketInstance.ReadTimeout = 0
      table.insert(sockets, SocketInstance)
      SocketInstance.EventHandler = SocketHandler
      clientSock:Connect(Controls.output_IP.String,
        tonumber(Controls.output_port.String) or tonumber(Controls.port.String))
    end
    server:Listen(Controls.port.String) -- This listen port is opened on all network interfaces





    ---====================CLIENT
    clientSock = TcpSocket.New()
    clientSock.ReadTimeout = 0
    clientSock.WriteTimeout = 0


    clientSock.Connected = function(sock)
      addText("TCP Connected", MessageFormat.STATUS)

      -- Stop the pending buffer timer
      pendingBufferTimer:Stop()

      -- Send any pending data
      if #pendingBuffer > 0 then
        addText(string.format("Sending %d buffered messages", #pendingBuffer), MessageFormat.STATUS)
        for _, data in ipairs(pendingBuffer) do
          clientSock:Write(data)
        end
        pendingBuffer = {} -- Clear the buffer
      end
    end
    clientSock.Reconnect = function(sock)
      addText("TCP Reconnecting", MessageFormat.STATUS)
    end
    clientSock.Data = function(sock)
      local data = sock:Read(sock.BufferLength)
      addText(data, MessageFormat.INCOMING)
      if sockets[1] and sockets[1].IsConnected then
        sockets[1]:Write(data)
      end
    end
    clientSock.Closed = function(clientSock)
      addText("TCP Closed", MessageFormat.STATUS)
      RemoveSocketsFromTable()
    end
    clientSock.Error = function(sock, err)
      addText("TCP Error: " .. err, MessageFormat.ERROR)
      RemoveSocketsFromTable()
    end
    clientSock.Timeout = function(sock, err)
      addText("TCP Timeout: " .. err, MessageFormat.ERROR)
      RemoveSocketsFromTable()
    end
  end

  function testMessages()
    local shortMessage = "Brief test."
    local mediumMessage = "This is a medium length message that might wrap once on the display."
    local longMessage =
    "This is a much longer message that will definitely need to wrap multiple times on the display. It contains more words and will demonstrate how the text wrapping handles longer content in a natural way."

    local baseTime = os.time({
      year = 2024,
      month = 1,
      day = 1,
      hour = 12,
      min = 0,
      sec = 0
    })

    -- Add variety of messages with different formats
    local testMessages = {
      { offset = 0,  text = ">> Sending connection request",         format = MessageFormat.OUTGOING },
      { offset = 1,  text = "<< Connection accepted",                format = MessageFormat.INCOMING },
      { offset = 2,  text = "Connection established successfully",   format = MessageFormat.STATUS },
      { offset = 3,  text = ">> " .. shortMessage,                   format = MessageFormat.OUTGOING },
      { offset = 4,  text = "<< Server response: " .. mediumMessage, format = MessageFormat.INCOMING },
      { offset = 5,  text = "Authentication required",               format = MessageFormat.STATUS },
      { offset = 6,  text = "Connection timeout - retry in 30s",     format = MessageFormat.ERROR },
      { offset = 7,  text = ">> " .. longMessage,                    format = MessageFormat.OUTGOING },
      { offset = 8,  text = "<< Processing request",                 format = MessageFormat.INCOMING },
      { offset = 9,  text = "Invalid credentials",                   format = MessageFormat.ERROR },
      { offset = 10, text = "System status: Online",                 format = MessageFormat.STATUS },
      { offset = 11, text = ">> Requesting data update",             format = MessageFormat.OUTGOING },
      { offset = 12, text = "<< Update complete",                    format = MessageFormat.INCOMING },
      { offset = 13, text = "Memory usage critical",                 format = MessageFormat.ERROR },
      { offset = 14, text = ">> Closing connection",                 format = MessageFormat.OUTGOING },
      { offset = 15, text = "Connection terminated",                 format = MessageFormat.STATUS }
    }

    -- Add messages with incrementing timestamps
    for _, msg in ipairs(testMessages) do
      addText(msg["text"], msg["format"])
    end
  end

  -- Add button handlers
  Controls.capture.EventHandler = function()
    if Controls.capture.Boolean then
      resetScroll()
      addText("Capture Enabled", MessageFormat.STATUS)
      Controls.ScrollUp.IsInvisible = true
      Controls.ScrollDown.IsInvisible = true
      Controls.ScrollToBottom.IsInvisible = true
    else
      addText("Capture Disabled", MessageFormat.STATUS)
      Controls.ScrollUp.IsInvisible = false
      Controls.ScrollDown.IsInvisible = false
      Controls.ScrollToBottom.IsInvisible = false
    end
  end

  -- Add validation function
  local function validateParams()
    -- Check if port is a valid number
    local inputPort = tonumber(Controls.port.String)
    if not inputPort or inputPort < 1 or inputPort > 65535 then
      return false
    end

    -- Check if output IP is valid format
    local ip = Controls.output_IP.String
    if not ip or ip == "" then
      return false
    end

    -- Basic IP format validation
    local parts = {}
    for part in ip:gmatch("%d+") do
      local num = tonumber(part)
      if not num or num < 0 or num > 255 then
        return false
      end
      table.insert(parts, num)
    end
    if #parts ~= 4 then
      return false
    end

    -- If output port is specified, validate it
    if Controls.output_port.String ~= "" then
      local outputPort = tonumber(Controls.output_port.String)
      if not outputPort or outputPort < 1 or outputPort > 65535 then
        return false
      end
    end

    return true
  end

  -- Modify reinit function
  function reinit()
    RemoveSocketsFromTable()
    if clientSock then
      clientSock:Disconnect()
      clientSock = nil
    end
    if server then
      server:Close()
    end

    if validateParams() then
      init()
    else
      addText("Enter valid port and destination IP to begin.", MessageFormat.STATUS)
    end
  end

  -- Add handler for filter text changes
  Controls.filter.EventHandler = function()
    currentFilter = Controls.filter.String
    currentScrollPos = 0 -- Reset scroll position when filter changes
    updateDisplay()
  end


  Controls.port.EventHandler = reinit
  Controls.output_IP.EventHandler = reinit
  Controls.output_port.EventHandler = reinit


  -- Initial startup
  Controls.capture.Boolean = true

  if validateParams() then
    init()
  else
    addText("Enter valid port and destination IP to begin.", MessageFormat.STATUS)
  end
end
